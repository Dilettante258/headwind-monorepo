{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build:wasm": {
      "outputs": ["wasm-pkg/**", "wasm/**"],
      "inputs": [
        "../../crates/wasm/src/**",
        "../../crates/wasm/Cargo.toml",
        "../../crates/transform/src/**",
        "../../crates/transform/Cargo.toml",
        "../../crates/core/src/**",
        "../../crates/core/Cargo.toml"
      ]
    },
    "build": {
      "dependsOn": ["^build", "build:wasm"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["build:wasm"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    }
  }
}
